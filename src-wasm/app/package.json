{
  "license": "MIT",
  "scripts": {
    "build": "wasm-bindgen ../../target/wasm32-unknown-unknown/release/app.wasm --out-dir ../../pkg --target web",
    "build:dev": "wasm-bindgen ../../target/wasm32-unknown-unknown/debug/app.wasm --out-dir ../../pkg --target web",
    "watch": "cargo watch -w .. -x build -s 'yarn build:dev'",
    "watch:prod": "cargo watch -w .. -x 'build --release' -s 'yarn build'",
    "clean": "rm -rf ./pkg"
  }
}
