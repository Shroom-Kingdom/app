{
  "license": "MIT",
  "scripts": {
    "build": "wasm-pack build --release --target web --out-dir ../../pkg",
    "build:dev": "wasm-pack build --dev --target web --out-dir ../../pkg",
    "watch": "cargo watch -w src -s 'wasm-pack build --dev --target=web --out-dir ../../pkg'",
    "watch:prod": "cargo watch -w src -s 'wasm-pack build --release --target=web --out-dir ../../pkg'",
    "clean": "rm -rf ./pkg"
  }
}
