{
  "name": "app",
  "version": "0.1.0",
  "repository": "git@github.com:Shroom-Kingdom/app.git",
  "author": "Mario Reder <mario.reder@pm.me>",
  "license": "MIT",
  "scripts": {
    "start": "yarn clean && yarn --cwd src-wasm/app build:dev && run-p serve watch",
    "start:prod": "yarn clean && yarn --cwd src-wasm/app build:dev && run-p serve:prod watch:prod",
    "serve": "webpack-dev-server --host 0.0.0.0 -d --env=dev",
    "serve:prod": "webpack-dev-server --host 0.0.0.0 -p --env=prod",
    "build": "yarn clean && yarn --cwd src-wasm/app build && webpack --env=prod",
    "watch": "yarn --cwd src-wasm/app watch",
    "watch:prod": "yarn --cwd src-wasm/app watch:prod",
    "deploy": "gh-pages --dist dist",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "clean": "rm -rf ./dist && yarn --cwd src-wasm/app clean"
  },
  "dependencies": {
    "detect-browser": "^5.2.0",
    "react": "^17",
    "react-dom": "^17",
    "styled-jsx": "^3.4.7",
    "ts-pattern": "^3.2.4"
  },
  "devDependencies": {
    "@babel/core": "^7",
    "@babel/plugin-syntax-dynamic-import": "^7",
    "@babel/plugin-transform-react-jsx": "^7",
    "@babel/plugin-transform-typescript": "^7",
    "@babel/polyfill": "^7",
    "@babel/preset-env": "^7",
    "@svgr/webpack": "^5.5.0",
    "@types/react": "^17",
    "@types/react-dom": "^17",
    "@typescript-eslint/eslint-plugin": "^4",
    "@typescript-eslint/parser": "^4",
    "babel-loader": "^8",
    "copy-webpack-plugin": "^9.0.1",
    "core-js": "3",
    "eslint": "^7",
    "eslint-config-prettier": "^6",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-ordered-imports": "^0",
    "eslint-plugin-prettier": "^3",
    "eslint-plugin-react": "^7",
    "file-loader": "^6",
    "html-webpack-plugin": "^4",
    "npm-run-all": "^4",
    "prettier": "^2",
    "ts-node": "^9",
    "typescript": "4.1",
    "webpack": "^5",
    "webpack-bundle-analyzer": "^4",
    "webpack-cli": "^3",
    "webpack-dev-server": "^3"
  }
}
