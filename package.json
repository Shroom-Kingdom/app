{
  "name": "app",
  "version": "0.1.0",
  "repository": "git@github.com:Shroom-Kingdom/app.git",
  "author": "Mario Reder <mario.reder@pm.me>",
  "license": "MIT",
  "scripts": {
    "start": "yarn clean && yarn --cwd src-wasm build:dev && run-p serve watch",
    "serve": "webpack-dev-server -d --env=dev",
    "build": "yarn clean && yarn --cwd src-wasm build && webpack --env=prod",
    "watch": "yarn --cwd src-wasm watch",
    "deploy": "gh-pages --dist dist",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "clean": "rm -rf ./dist && yarn --cwd src-wasm clean"
  },
  "dependencies": {
    "react": "^17",
    "react-dom": "^17"
  },
  "devDependencies": {
    "@babel/core": "^7",
    "@babel/plugin-syntax-dynamic-import": "^7",
    "@babel/plugin-transform-react-jsx": "^7",
    "@babel/plugin-transform-typescript": "^7",
    "@babel/polyfill": "^7",
    "@babel/preset-env": "^7",
    "@types/react": "^17",
    "@types/react-dom": "^17",
    "@typescript-eslint/eslint-plugin": "^4",
    "@typescript-eslint/parser": "^4",
    "babel-loader": "^8",
    "core-js": "3",
    "eslint": "^7",
    "eslint-config-prettier": "^6",
    "eslint-plugin-ordered-imports": "^0",
    "eslint-plugin-prettier": "^3",
    "eslint-plugin-react": "^7",
    "file-loader": "^6",
    "html-webpack-plugin": "^4",
    "npm-run-all": "^4",
    "prettier": "^2",
    "ts-node": "^9",
    "typescript": "4.1",
    "webpack": "^5",
    "webpack-bundle-analyzer": "^4",
    "webpack-cli": "^3",
    "webpack-dev-server": "^3"
  }
}
